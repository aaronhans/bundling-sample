<table>
  {{#if personnelMls.length}}
    {{#personnelMls}}
        <tr data-id="{{personnelMlsMapId}}">
            <td>{{mlsSourceLongName}}</td>
            <td>{{personnelMlsId}}</td>
            <td>
              {{#if agentFullName}}
                  {{agentFullName}}
              {{else}}
                  {{agentFirstName}} {{agentLastName}}
              {{/if}}
            <td><a href="#" class="icon-delete"></a></td>
        </tr>
    {{/personnelMls}}
{{else}}
   <tr><td colspan="4"><p>We can't find any matches for you! Select your MLS and try searching your name below.</p></td></tr>
{{/if}}
</table>
